#include <stdio.h>

void show_matrix(int matrix[3][3]){
    int lin = 0, col = 0;

    do{
        while(col < 3){
            printf("%d ",matrix[lin][col]);
            col++
        }
    printf("\n");

    col = 0;
    lin++;
    }while(lin < 3);
}

void main(){
    int sudoku[3][3] = {0};

    show_matrix(sudoku);


}